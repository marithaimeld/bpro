public class WOMethod
{
	public static void cetakGaris()
	{
		for(int i=0;i<35;i++)
			System.out.print("*");
			System.out.println();
	}
	
	public static void main (String []argv)
	{
		cetakGaris();
			System.out.println("View DATA Customer");
		cetakGaris();
			System.out.println("id\t Nama\t Alamat");
		cetakGaris();
	}
}
